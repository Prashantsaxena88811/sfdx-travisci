    <aura:if isTrue="{!v.openFilterModal}">
    <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
        <div class="slds-modal__container">
          <header class="slds-modal__header">
            <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="Close" onclick="{!c.closeFilterModal}" >
                <lightning:icon iconName="utility:close" alternativeText="close" title="close" />
                <span class="slds-assistive-text">Close</span>
              </button>
          
            <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate">SAVE NEW FILTER</h2>
            <p class="slds-m-top_x-small">
                Select existing filter or enter a name to save your filter
            </p>
          </header>
          <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
            
        
            <lightning:combobox aura:id="existingFilters" 
                                name="Existing Filter" 
                                label="Existing Filter" 
                                value="{!v.selectedExistingFilter}" 
                                placeholder="Select Existing Filter" 
                                options="{! v.existingFilters }"
                                onchange="{!c.existingfilterNameChange}" />
            <br/>

          <lightning:input aura:id="newFilterName" name="filterName" value="{!v.selectedNewFilter}" label="FilterName" onchange="{!c.newfilterNameChange}" />

          </div>
          <footer class="slds-modal__footer">
            <button class="slds-button slds-button_neutral" onclick="{!c.closeFilterModal}">Cancel</button>
            <button class="slds-button slds-button_brand" onclick="{!c.submitFilter}" disabled="{!v.selectedMyFilter.length == 0}">Submit</button>
          </footer>
        </div>
      </section>
      <div class="slds-backdrop slds-backdrop_open"></div>
      </aura:if>

