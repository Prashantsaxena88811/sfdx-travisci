<aura:component controller="CustomLookupController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="optionsType" type="List" access="public"/>
    <aura:attribute name="optionscurrency" type="List" access="public"/>
    
    <aura:attribute name="selectedValueType" type="String" access="public"/>
    <aura:attribute name="selectedValueCurrency" type="String" access="public"/>
    
   <aura:handler name="init" value="{!this}" action="{!c.init}" />
   <!--vvvvvvvvvvvvvv====> {!v.recordId}
    selectedValueType = {!v.selectedValueType}
    selectedValueCurrency = {!v.selectedValueCurrency}-->
    <div> <lightning:select name="OpportunityType" label="Opportunity Type:" aura:id="OpportunityType" 
                            value="{!v.selectedValueType}">
        <aura:iteration items="{!v.optionsType}" var="option">
            <option text="{!option.label}" value="{!option.value}" selected="{!option.selected}"/>
        </aura:iteration>
        </lightning:select>
    </div>
    <div> <lightning:select name="OpportunityCurrency" label="Opportunity Currency:" 
                            aura:id="OpportunityCurrency" value="{!v.selectedValueCurrency}">
        <aura:iteration items="{!v.optionscurrency}" var="option">
            <option text="{!option.label}" value="{!option.value}" selected="{!option.selected}"/>
        </aura:iteration>
        </lightning:select>
    </div>
</aura:component>